<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5p %d{MM-dd HH:mm:ss,SSS} %m  (%F:%L)\n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="org.apache.ibatis" level="DEBUG">
            <AppenderRef ref="Console"/>
        </Logger>
        <Root level="DEBUG">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>